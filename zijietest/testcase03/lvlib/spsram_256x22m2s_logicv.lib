//* Template Version : S_09_74801   
MemoryTemplate (spsram_256x22m2s) {

      CellName        : spsram_256x22m2s;
      Algorithm       : SMarchCHKBvcd;
      logicalPorts    : 1rw;
      NumberOfWords   : 256;
      MinHold         : 0.5;
      MilliWattsPerMegaHertz : 0.003;
      TransparentMode : None; 		//User options for DFT:
      					// AsyncMux: 	for low speed scan
					// SyncMux: 	for at-speed scan
					// AsyncWriteThru: if there is AWT in this memory
      

      Port (CLK)  {
          Function: Clock;
      }
      Port (CEB)  {
                    Function: Select;
	            Polarity: ActiveLow;	

      }

      Port (WEB) {
            Function: WriteEnable; 
	    Polarity: ActiveLow;

      }

      Port (BWEB[21:0]) {
            Function: GroupWriteEnable; 
	    Polarity: ActiveLow;

      }

      Port (D[21:0]) { 
          Function: Data; 
          Direction:   Input;
      }
      Port (Q[21:0]) { 
          Function: Data; 
          Direction:   Output; 
      }
      port (A[7:0]) {
          function:    address;
      }







      Port (RTSEL[1:0]) {
          Direction : Input;
	  function  : None;
	  SafeValue : 2'b01;
      }
      Port (WTSEL[1:0]) {
          Direction : Input;
	  function  : None;
	  SafeValue : 2'b00;
      }


// 128 rows by 2 columns.
      AddressCounter {
          Function ( Address ) {
	      LogicalAddressMap {
                  ColumnAddress[0:0]:Address[0:0];  // number of columns
		  RowAddress[6:0]:Address[7:1];  // number of rows
	      }
	  }
	  Function ( ColumnAddress ) {
              CountRange[0:1];
	  }
	  Function  ( RowAddress ) {
              CountRange[0:127];
	  }
      }

      OperationSet : syncWR;
      bitGrouping : 1;
}
