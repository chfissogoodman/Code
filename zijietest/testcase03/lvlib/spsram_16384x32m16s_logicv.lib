//* Template Version : S_09_74801   
MemoryTemplate (spsram_16384x32m16s) {

      CellName        : spsram_16384x32m16s;
      Algorithm       : SMarchCHKBvcd;
      logicalPorts    : 1rw;
      NumberOfWords   : 16384;
      MinHold         : 0.5;
      MilliWattsPerMegaHertz : 0.021;
      TransparentMode : None; 		//User options for DFT:
      					// AsyncMux: 	for low speed scan
					// SyncMux: 	for at-speed scan
					// AsyncWriteThru: if there is AWT in this memory
      

      Port (CLK)  {
          Function: Clock;
      }
      Port (CEB)  {
                    Function: Select;
	            Polarity: ActiveLow;	

      }

      Port (WEB) {
            Function: WriteEnable; 
	    Polarity: ActiveLow;

      }

      Port (BWEB[31:0]) {
            Function: GroupWriteEnable; 
	    Polarity: ActiveLow;

      }

      Port (D[31:0]) { 
          Function: Data; 
          Direction:   Input;
      }
      Port (Q[31:0]) { 
          Function: Data; 
          Direction:   Output; 
      }
      port (A[13:0]) {
          function:    address;
      }







      Port (RTSEL[1:0]) {
          Direction : Input;
	  function  : None;
	  SafeValue : 2'b01;
      }
      Port (WTSEL[1:0]) {
          Direction : Input;
	  function  : None;
	  SafeValue : 2'b00;
      }


// 1024 rows by 16 columns.
      AddressCounter {
          Function ( Address ) {
	      LogicalAddressMap {
                  ColumnAddress[3:0]:Address[3:0];  // number of columns
		  RowAddress[0]:Address[4];  // number of rows
		  RowAddress[1]:Address[5];  // number of rows
		  RowAddress[2]:Address[7];  // number of rows
		  RowAddress[3]:Address[8];  // number of rows
		  RowAddress[4]:Address[9];  // number of rows
		  RowAddress[5]:Address[10];  // number of rows
		  RowAddress[6]:Address[11];  // number of rows
		  RowAddress[7]:Address[12];  // number of rows
		  BankAddress[0]:Address[6];  
		  BankAddress[1]:Address[13];  
	      }
	  }
	  Function ( ColumnAddress ) {
              CountRange[0:15];
	  }
	  Function  ( RowAddress ) {
              CountRange[0:255];
	  }
	  Function  ( BankAddress ) {
              CountRange[0:3];
	  }
      }

      OperationSet : syncWR;
      bitGrouping : 1;
}
